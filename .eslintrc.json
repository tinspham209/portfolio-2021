{
  "root": true,
  "extends": "next",
  "rules": {
    "no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ],
    "@next/next/no-document-import-in-page": "off",
    "react/no-unknown-property": ["error", { "ignore": ["jsx", "global"] }],

    "react/display-name": 0
  }
}
